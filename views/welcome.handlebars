    {{#each Location}}
    
    <div class="row info">
        <div class="col-sm-8">
            <div class="card">
              <div class="card-title">
                <h2 class="location">{{name}}</h2>
              </div>
                <div class="card-body">
                  <p class="card-text">{{description}}</p>
                  <button class="like" data-id="{{this.id}}"><i class="fas fa-thumbs-up"></i></button>
                  <button class="review" data-id="{{this.id}}">Review</button>
                </div>
              </div>
        </div>
        <div class="col-sm-4">
          <img src={{picture_url}} alt="Picture Here">
        </div>
      </div>


  {{/each}}